{
  "manifest_version": 3,
  "name": "WhatsApp Privacy Extension",
  "version": "1.0",
  "description": "Enhance your WhatsApp privacy settings",
  "action": {
    "default_popup": "index.html",
    "default_icon":"image.png"
  },
  "icons": {
    "128": "image.png"
  },
 
   "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "*://web.whatsapp.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*"],
      "js": ["content.js"],
       "run_at": "document_idle"
    }
  ]
 
}